{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"rootDir": "./src",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"include": [
		"src/**/*.ts",
	],
	// Exclure les fichiers et dossiers sp√©cifiques
	"exclude": [
		"node_modules",
		"dist"
	]
}